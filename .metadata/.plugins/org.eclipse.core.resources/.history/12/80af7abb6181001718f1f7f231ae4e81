package com.udemy.ForkJoinAlgo;

public class SequentialMaxFind {
	
	private int nums[];
	private int highIndex;
	
	public SequentialMaxFind(int[] nums, int highIndex) {
		super();
		this.nums = nums;
		this.highIndex = highIndex;
	}
	
	
	public int SequentialMaxFind() {
		int max = nums[0];
		
		for(int i=0;i< highIndex; i++)
		{
			if(nums[i]> max)
			{
				max= nums[i];
			}
		}
		return max;
	}
}
