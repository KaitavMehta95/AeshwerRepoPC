package com.udemyConcurreny;

public class SynchronizationApp {
	
	public void process()
	{
		Thread t1 = new Thread(new Runnable() 
		{
			
			@Override
			public void run() 
			{
				for(int i=0;i<10;i++){System.out.println("Runner1");}				
			}
		}
		);
		
	}
	

	public static void main(String[] args) {
		
	}
}
