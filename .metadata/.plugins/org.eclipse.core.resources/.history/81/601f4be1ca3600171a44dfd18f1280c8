
public abstract class Duck {

	FlyBehaviour flyBehaviour;
	QuackBehaviour quackBehaviour;
	
	
	
	abstract void display();
	
	void swim()
	{
		System.out.println("All Ducks can Swim");
	}
	
	void performFlying()
	{
		flyBehaviour.performFly();	
	}
	
	void performQucking()
	{
		quackBehaviour.performQuack();
	}
	
}
