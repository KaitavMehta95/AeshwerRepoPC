package com.udemy.ForkJoinAlgo;

public class SequentialMaxFind {
	
	private long nums[];
	private long highIndex;
	
	public SequentialMaxFind(long[] nums, long highIndex) {
		super();
		this.nums = nums;
		this.highIndex = highIndex;
	}
	
	
	public long SequentialMaxFind() {
		long max = nums[0];
		
		for(long i=0;i< highIndex; i++)
		{
			if(nums[i]> max)
			{
				max= nums[i];
			}
		}
		return max;
	}
}
