package com.udemyConcurreny;

public class ProducerConsumerWithLOck {
	

	Thread t1 = new Thread(new Runnable(){
		@Override
		public void run() 
		{
				increment();
		}
	}
	);
	
	Thread t2 = new Thread(new Runnable(){
		@Override
		public void run() 
		{
				increment();
		}
	}
	);
	
	t1.start();
	t2.start();
	
	try {
		t1.join();
		t2.join();
	} catch (InterruptedException e) {
		e.printStackTrace();
	}
	
}

