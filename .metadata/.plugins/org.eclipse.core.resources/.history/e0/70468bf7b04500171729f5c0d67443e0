package ArraysProblems;

public class FindFixedPointInArray {

    
    //main function
    public static void main(String args[])
    {
        int arr[] = {-10, -1, 0, 3 , 10, 11, 30, 50, 100};
        int n = arr.length;
        System.out.println("Fixed Point is "+ binarySearch(arr,0, n-1));        
    } 
}
