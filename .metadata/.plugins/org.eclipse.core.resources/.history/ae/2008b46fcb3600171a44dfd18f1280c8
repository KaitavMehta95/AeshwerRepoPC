package ClientSide;
import Duck;
import ModelDuck;
import Squeek;

public class TestSimulationDuck {

	
	public static void main(String[] args) { 
		
		Duck modelDuck = new ModelDuck();
		modelDuck.display();
		modelDuck.performFlying();
		modelDuck.performQucking();
		modelDuck.setquackBehaviour(new Squeek());
		modelDuck.performQucking();
		
		
	}
	
}
