
public class WhichMethodCalled {
	
	class Actor
	{
		void act()
		{
			System.out.println("Actor act");
		}
		void dance()
		{
			System.out.println("Actor dance");
		}
	}
	
	class hero extends Actor
	{
		void fuck()
		{
			System.out.println("Hero fuck");
		}
		@Override
		void dance()
		{
			System.out.println("Hero dance");
		}
	}
	

	public static void main(String[] args) {
		Actor actor = new hero();

	}

}
