/*
 * Minimum number of coin to form total value
 * 
 * Total : 5 by 1,2,3 coins
 */
package DynamicProgrammingPractise;

public class CoinChange {

	public static void main(String[] args) {
		 
			int arr[] = {1, 2, 3};
	        int m = arr.length;
	        int n = 4;
	        System.out.println(countWays(arr, m, n));
	}

}
